<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="250.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.TableOverviewController">
   <children>
      <BorderPane prefHeight="500.0" prefWidth="600.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <center>
            <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn prefWidth="30.0" text="ID">
                    <cellValueFactory><PropertyValueFactory property="iD" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="70.0" text="Dur.">
                    <cellValueFactory><PropertyValueFactory property="duration" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="Budget">
                    <cellValueFactory><PropertyValueFactory property="budget" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="90.0" text="Planned progress">
                    <cellValueFactory><PropertyValueFactory property="plannedProgress" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="90.0" text="Current progress">
                    <cellValueFactory><PropertyValueFactory property="currentProgress" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="PV">
                    <cellValueFactory><PropertyValueFactory property="pV" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="40.0" text="AC">
                    <cellValueFactory><PropertyValueFactory property="aC" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="55.0" text="EV">
                    <cellValueFactory><PropertyValueFactory property="eV" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="55.0" text="CV">
                    <cellValueFactory><PropertyValueFactory property="cV" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="55.0" text="SV">
                    <cellValueFactory><PropertyValueFactory property="sV" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="55.0" text="CPI">
                    <cellValueFactory><PropertyValueFactory property="cPI" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="55.0" text="SPI">
                    <cellValueFactory><PropertyValueFactory property="sPI" />
                    </cellValueFactory>
                </TableColumn>
              </columns>
              <items>
                  <FXCollections fx:factory="observableArrayList">
                      
                
                  </FXCollections>
              </items>
            </TableView>
         </center>
         <bottom>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="edit" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleEdit" text="Edit..." GridPane.halignment="CENTER">
                     <GridPane.margin>
                        <Insets right="20.0" />
                     </GridPane.margin>
                  </Button>
               </children>
            </GridPane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
