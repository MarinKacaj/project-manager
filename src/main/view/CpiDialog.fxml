<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="300.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/8" fx:controller="main.CpiDialogController">
    <children>
        <GridPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <FlowPane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <Label text="CPI (1)">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </Label>
                        <TextField fx:id="firstCPI" prefWidth="80.0">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </TextField>
                    </children>
                </FlowPane>
                <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                    <children>
                        <Label text="CPI (2)">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </Label>
                        <TextField fx:id="secondCPI" prefWidth="80.0">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </TextField>
                    </children>
                </FlowPane>
                <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
                    <children>
                        <Label text="CPI (3)">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </Label>
                        <TextField fx:id="thirdCPI" prefWidth="80.0">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </TextField>
                    </children>
                </FlowPane>
                <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
                    <children>
                        <Label text="SPI (1)">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </Label>
                        <TextField fx:id="firstSPI" prefWidth="80.0">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </TextField>
                    </children>
                </FlowPane>
                <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <children>
                        <Label text="SPI (2)">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </Label>
                        <TextField fx:id="secondSPI" prefWidth="80.0">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </TextField>
                    </children>
                </FlowPane>
                <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <children>
                        <Label text="SPI (3)">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </Label>
                        <TextField fx:id="thirdSPI" prefWidth="80.0">
                            <FlowPane.margin>
                                <Insets left="20.0" top="30.0"/>
                            </FlowPane.margin>
                        </TextField>
                    </children>
                </FlowPane>
                <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2"
                          GridPane.rowIndex="3">
                    <children>
                        <Button fx:id="save" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleSave"
                                prefWidth="70.0" text="Ruaj">
                            <FlowPane.margin>
                                <Insets right="10.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="cancel" mnemonicParsing="false" onAction="#handleCancel" prefWidth="70.0"
                                text="Cancel">
                            <FlowPane.margin>
                                <Insets left="10.0"/>
                            </FlowPane.margin>
                        </Button>
                    </children>
                </FlowPane>
            </children>
        </GridPane>
    </children>
</AnchorPane>
