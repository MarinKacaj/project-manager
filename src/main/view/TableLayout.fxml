<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.TableOverviewController">
	<children>
		<GridPane prefHeight="230.0" prefWidth="600.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="5.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<TableView fx:id="activityTable" editable="true" prefHeight="180.0" prefWidth="200.0" GridPane.vgrow="ALWAYS">
					<columns>
						<TableColumn fx:id="nameColumn" prefWidth="100.0" text="Name">
						</TableColumn>
						<TableColumn fx:id="idColumn" prefWidth="30.0" text="ID">
						</TableColumn>
						<TableColumn fx:id="durationColumn" prefWidth="70.0" text="Dur.">
						</TableColumn>
						<TableColumn fx:id="budgetColumn" prefWidth="75.0" text="Budget">
						</TableColumn>
						<TableColumn fx:id="unitColumn" prefWidth="75.0" text="Unit">
						</TableColumn>
                                                <TableColumn fx:id="staticAmountColumn" prefWidth="75.0" text="Static Amount">
						</TableColumn>
						<TableColumn fx:id="plannedAmountColumn" prefWidth="75.0" text="Planned Amount">
						</TableColumn>
						<TableColumn fx:id="priceColumn" prefWidth="75.0" text="Price">
						</TableColumn>
						<TableColumn fx:id="currentAmountColumn" prefWidth="75.0" text="Current Amount">
						</TableColumn>
						<TableColumn fx:id="actualAmountColumn" prefWidth="75.0" text="Actual Amount">
						</TableColumn>
						<TableColumn fx:id="currentProgressColumn" prefWidth="90.0" text="Current progress">
						</TableColumn>
						<TableColumn fx:id="pvColumn" prefWidth="75.0" text="PV">
						</TableColumn>
						<TableColumn fx:id="acColumn" prefWidth="40.0" text="AC">
						</TableColumn>
						<TableColumn fx:id="evColumn" prefWidth="55.0" text="EV">
						</TableColumn>
						<TableColumn fx:id="cvColumn" prefWidth="55.0" text="CV">
						</TableColumn>
						<TableColumn fx:id="svColumn" prefWidth="55.0" text="SV">
						</TableColumn>
						<TableColumn fx:id="cpiColumn" prefWidth="55.0" text="CPI">
						</TableColumn>
						<TableColumn fx:id="spiColumn" prefWidth="55.0" text="SPI">
						</TableColumn>
					</columns>
					<items>
						<FXCollections fx:factory="observableArrayList">

						</FXCollections>
					</items>
					<columnResizePolicy>
						<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
					</columnResizePolicy>
				</TableView>
				<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
					<children>
						<Button id="button" fx:id="add" mnemonicParsing="false" onAction="#handleAdd" text="Shto">
							<HBox.margin>
								<Insets right="20.0" />
							</HBox.margin>
						</Button>
						<Button id="button" fx:id="delete" mnemonicParsing="false" onAction="#handleDelete" text="Fshij">
							<HBox.margin>
								<Insets left="20.0" />
							</HBox.margin>
						</Button>
					</children>
					<GridPane.margin>
						<Insets top="5.0" />
					</GridPane.margin>
				</HBox>
			</children>
		</GridPane>
	</children>
</AnchorPane>
